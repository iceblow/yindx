.leftNav .opration .active{
	background: #273051;
	border-left:4px solid #18C5BF;
	padding-left: 17px;
	color:#fff;
} 

.leftNav .opration .activeP{
	color:#fff;
} 

.leftNav .opration p:hover {
	color: #fff;
} 

.leftNav .opration p img{
	float: left;
	margin-top:11px;
	margin-right:16px;
}

.leftNav .opration p .fanhui{
	float: right;
	margin-top: 19px;
	margin-right: 38px;
}

.leftNav .opration ul {
	display: none;
}

.leftNav .opration ul li {
	line-height: 39px;
	padding-left: 48px;
	cursor: pointer;
	font-size: 14px;
}

.leftNav .opration ul .active {
	background: #273051;
	border-left:4px solid #18C5BF;
	padding-left: 44px;
	color:#fff;
}

.leftNav .opration ul li:hover {
	background: #273051;
	border-left:4px solid #18C5BF;
	padding-left: 44px;
	color:#fff;
}
 

/* 自定义表单 */
input:disabled{
	border:1px solid #f5f5f5;
	background-color:#f9f9f9;
	color:#999;
}

.disabled {
	background:rgb(246,246,246);
}
