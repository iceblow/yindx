@charset "utf-8";
* {
	padding: 0;
	margin: 0;
	font-family: "微软雅黑";
}

body,html {
	height: 100%;
	font-size: 14px;
	border: none;
	overflow-x: hidden;
}

ul,li {
	list-style: none;
}

/* 顶部 */
.topNav {
	position: fixed;
	top:0px;
	height: 79px;
	width: 100%;
	color: #89a1b7;
	z-index: 99;
	font-size:14px;
 	background: #fff;
 	border-bottom: 1px solid #F4F4F6;
}

/* 标题 */
.topNav .title {
	width: 200px;
	height: 80px;
	text-align:center;
	line-height:80px;
	float: left;
	letter-spacing: 2px;
	background:#18C5BF;
	color:#fff;
	font-size:20px;
	font-family:"方正兰亭黑简体";
}

/* 搜索 */
.topNav .inputBox{
	float: left;
	height: 32px;
	line-height: 80px;
	margin-left: 23px;
	margin-top:24px;
	width:220px;
	position: relative;
}

.topNav .inputBox .icon{
	position: absolute;
    top: 7px;
    right: 15px;
}

.topNav .inputBox .input{
	position: absolute;
    top: 0px;
    left: 0px;
    width:100%;
    height:100%;
    line-height:32px;
    padding-left:15px;
    padding-right:33px;
    box-sizing:border-box;
    font-size:12px;
    color:#c7d5e1;
    overflow: hidden;
	white-space:nowrap;
}

.topNav .inputBox .input:focus{
	outline:none;
}

/* 操作 */
.topNav .opration{
	float: right;
	height: 100%;
	line-height: 80px;
	width:124px;
	border-left:1px solid #eff4f9;
}

.topNav .opration img{
	float: left;
	margin-top:31px;
	margin-right:10px;
	margin-left:19px;
}

/*左边导航栏*/
.leftNav {
	position: fixed;
	top: 80px;
	height: 2000px;
	width: 200px;
	background: #333C5B;
	color: #fff;
}

/* 操作 */
.leftNav .opration {
	width: 100%;
	height: 100%;
	overflow: auto;
}

.leftNav .opration p {
	line-height: 50px;
	padding-left: 55px;
	cursor: pointer;
	font-size: 14px;
	color: #B0B9DA;
	background-position: 0;
}

.leftNav .opration .pandect {
	line-height: 40px;
	padding-left: 21px;
	cursor: pointer;
	font-size: 14px;
	color: #b1badc;
	margin-top:10px;
}

.leftNav .opration #alertHandel {
	background: url(../images/alertHandel_gray.png);
}

.leftNav .opration #alertPublish {
	background: url(../images/alertPublish_gray.png);
}

.leftNav .opration #notice {
	background: url(../images/notice_gray.png);
}

.leftNav .opration #user {
	background: url(../images/user_gray.png);
}

.leftNav .opration #equipment {
	background: url(../images/equipment_gray.png);
}

.leftNav .opration #query {
	background: url(../images/query_gray.png);
}

.leftNav .opration #systemSetup {
	background: url(../images/systemSetup_gray.png);
}

