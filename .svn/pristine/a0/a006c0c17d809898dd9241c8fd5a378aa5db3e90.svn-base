	float:left;
	height:40px;
	border-bottom:1px solid #F0F4F5;
	box-sizing:border-box;
	line-height:40px;
	padding:0 40px; 
}

.right .tableBox .table .tbody .tr .td{
	float:left;
	height:40px;
	line-height:40px;
	font-size:16px;
	cursor:pointer;
}

.right .tableBox .table .tbody .tr .tdr{
	float:right;
	font-size:14px;
}

.right .tableBox  a{ 
	text-decoration: none;
	color: #42a6eb;
	cursor:pointer;
}

.right .tableBox a:hover{ 
	color: #3497db;
	text-decoration: underline;
}

.right .tableBox .lineStr {
    color: #b2b2b2;
